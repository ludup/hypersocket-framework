<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE xml>
<propertyTemplates>
	<propertyStore id="databaseConfiguration"
		type="com.hypersocket.properties.DatabasePropertiesFileConfigurationStore">
		<filename>${hypersocket.conf}/database.properties</filename>
	</propertyStore>
	<propertyStore id="schedulerConfiguration"
		type="com.hypersocket.properties.SchedulerPropertiesFileConfigurationStore"
		create="true">
		<filename>${hypersocket.conf}/quartz.properties</filename>
	</propertyStore>
	<propertyCategory resourceKey="category.authentication"
		 resourceBundle="AuthenticationService" weight="100" hidden="true">
		<property resourceKey="auth.chooseRealm" inputType="switch"
			defaultValue="true" weight="0" />
	</propertyCategory>
	<propertyCategory resourceKey="category.email"
		resourceBundle="EmailService" weight="200">
			<property resourceKey="smtp.on" inputType="switch" weight="0" defaultValue="true"/>
	</propertyCategory>
	<propertyCategory resourceKey="category.security" resourceBundle="HypersocketServer" weight="400">
		<property resourceKey="security.knownHosts" inputType="hidden" defaultValue="" weight="0" cache="false"/>
	    <property resourceKey="security.enableCSRFProtection" inputType="switch" defaultValue="true" weight="0"/>
	    <property resourceKey="security.xFrameOptionsEnabled" inputType="switch" defaultValue="true" weight="100"/>
	    <property resourceKey="security.strictTransportSecurity" inputType="switch" defaultValue="true" weight="200"/>
	    <property resourceKey="security.xFrameOptionsValue" inputType="select" defaultValue="SAMEORIGIN" options="DENY,SAMEORIGIN" weight="300"/>
	    <property resourceKey="security.sessionMaxAge" inputType="integer" defaultValue="365" weight="400"/>
	    <property resourceKey="security.enableXForwardedForDomainNameResolution" inputType="switch" defaultValue="false" weight="500"/>
		<property resourceKey="security.minGeneratedUid" inputType="text" minValue="0" maxValue="2147483647"
			allowEmpty="false" weight="500" defaultValue="2000" />
		<property resourceKey="security.minGeneratedGid" inputType="text" minValue="0" maxValue="2147483647"
			allowEmpty="false" weight="600" defaultValue="1000" />
		 <property resourceKey="userstack.accesskey" inputType="text"
			restart="false" weight="0" allowEmpty="true"/>
	</propertyCategory>
	<propertyCategory resourceKey="category.databaseSettings"
		resourceBundle="DatabaseService" weight="200">
		<property resourceKey="jdbc.vendor" inputType="select" defaultValue="MYSQL_LOCAL" options="MYSQL,MYSQL_LOCAL,POSTGRES,MSSQL,DERBY,H2" store="databaseConfiguration"
			allowEmpty="false" weight="100" />
		<property resourceKey="jdbc.database" inputType="text" store="databaseConfiguration"
			allowEmpty="false" weight="200" defaultValue="hypersocket" />	
		<property resourceKey="jdbc.host" inputType="text" store="databaseConfiguration" visibilityDependsOn="jdbc.vendor"
				  visibilityDependsValue="MSSQL|MYSQL|POSTGRES" allowEmpty="false" weight="300" defaultValue="localhost" />
		<property resourceKey="jdbc.port" inputType="integer" store="databaseConfiguration"  visibilityDependsOn="jdbc.vendor"
				  visibilityDependsValue="MSSQL|MYSQL|POSTGRES" allowEmpty="false" weight="400" defaultValue="3306" minValue="1" maxValue="65535"/>
		<property resourceKey="jdbc.username" inputType="text" store="databaseConfiguration"
			allowEmpty="true" weight="500" defaultValue="hypersocket" />
		<property resourceKey="jdbc.password" inputType="password" store="databaseConfiguration"
			allowEmpty="true" weight="600" defaultValue="hypersocket" />
		<property resourceKey="jdbc.maxPoolSize" inputType="integer" store="databaseConfiguration" 
				  allowEmpty="false" weight="700" defaultValue="100" minValue="1" maxValue="10000"/>		
		<property resourceKey="jdbc.numHelperThreads" inputType="integer" store="databaseConfiguration" 
				  allowEmpty="false" weight="800" defaultValue="0" minValue="0" maxValue="100"/>		
		<property resourceKey="jdbc.maxStatementsPerConnection" inputType="integer" store="databaseConfiguration" 
				  allowEmpty="false" weight="900" defaultValue="100" minValue="1" maxValue="1000"/>		
		<property resourceKey="jdbc.acquireRetryDelay" inputType="integer" store="databaseConfiguration" 
				  allowEmpty="false" weight="1000" defaultValue="1000" minValue="1" maxValue="100000"/>		
		<property resourceKey="jdbc.acquireRetryAttempts" inputType="integer" store="databaseConfiguration" 
				  allowEmpty="false" weight="1100" defaultValue="10" minValue="1" maxValue="100"/>
		<property resourceKey="jdbc.timeZone" inputType="select" defaultValue="UTC" store="databaseConfiguration" allowEmpty="false" weight="1200" 
				  emptySelectionAllowed="true" url="configuration/timeZone/list"  visibilityDependsOn="jdbc.vendor"
				  visibilityDependsValue="MYSQL|MYSQL_LOCAL"/>
		<property resourceKey="jdbc.localSocket" inputType="text" defaultValue="/var/run/mysqld/mysqld.sock" store="databaseConfiguration" allowEmpty="false" weight="1300" 
				  emptySelectionAllowed="false" visibilityDependsOn="jdbc.vendor"
				  visibilityDependsValue="MYSQL_LOCAL"/>
	</propertyCategory>
	<propertyCategory resourceKey="category.schedulerSettings"
		resourceBundle="SchedulerService" weight="300">
		<property resourceKey="org.quartz.threadPool.threadCount" inputType="integer" store="schedulerConfiguration" 
				  allowEmpty="false" weight="700" defaultValue="0" minValue="0" maxValue="1000"/>	
		<property resourceKey="scheduler.reconcileDisabled" inputType="switch"
			defaultValue="false" weight="20" />	
	</propertyCategory>
	<propertyCategory resourceKey="category.dictionary"
		resourceBundle="DictionaryResourceService" weight="700" group="dictionary">
		<property resourceKey="dictionary.minWordLength" inputType="integer"
			defaultValue="4" weight="10" minValue="1" maxValue="255" />
		<property resourceKey="dictionary.matchAlphaOnly" inputType="switch"
			defaultValue="false" weight="20" />
		<property resourceKey="dictionary.caseInsenstive" inputType="switch"
			defaultValue="true" weight="30" />
		<property resourceKey="dictionary.blacklistBuiltIn" inputType="switch"
			defaultValue="true" weight="50" />
	</propertyCategory>
	
	
	<propertyCategory resourceKey="category.dictionaryApi"
		resourceBundle="DictionaryResourceService" weight="900" group="dictionary">	
		<property resourceKey="dictionary.blacklistApiCall" inputType="switch"
			defaultValue="false" weight="100" />
		<property resourceKey="dictionary.authentication" inputType="select" defaultValue="NONE" options="NONE,BASIC,NTLM"
			allowEmpty="false" weight="125" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.username" inputType="text" 
			allowEmpty="true" weight="150" defaultValue="" visibilityDependsOn="dictionary.blacklistApiCall,dictionary.authentication" visibilityDependsValue="true,BASIC|NTLM"/>/>
		<property resourceKey="dictionary.password" inputType="password"
			allowEmpty="true" weight="175" defaultValue="" visibilityDependsOn="dictionary.blacklistApiCall,dictionary.authentication" visibilityDependsValue="true,BASIC|NTLM"/>/>
		<property resourceKey="dictionary.method" inputType="select"
			options="GET, POST" weight="200" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.certificate" inputType="switch"
			defaultValue="" weight="300" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.url" inputType="text"
			defaultValue="" weight="400" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.variables" inputType="namePairs"
			weight="500" urlEncode="false" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.headers" inputType="namePairs"
			weight="600" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.responseList" inputType="multipleTextInput"
			weight="700" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"
			defaultValue="200"/>
		<property resourceKey="dictionary.responseContent" inputType="text"
			defaultValue="" weight="800" visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
	</propertyCategory>
			
	<propertyCategory resourceKey="category.dictionaryApiLogon"
		resourceBundle="DictionaryResourceService" weight="1000" group="dictionary">
		<property resourceKey="dictionary.logonRequired" inputType="switch"
			defaultValue="false" weight="100"  visibilityDependsOn="dictionary.blacklistApiCall" visibilityDependsValue="true"/>
		<property resourceKey="dictionary.logonAuthentication" inputType="select" defaultValue="NONE" options="NONE,BASIC,NTLM"
			allowEmpty="false" weight="125" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
		<property resourceKey="dictionary.logonUsername" inputType="text" 
			allowEmpty="true" weight="150" defaultValue="" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall,dictionary.authentication" visibilityDependsValue="true,true,BASIC|NTLM"/>/>
		<property resourceKey="dictionary.logonPassword" inputType="password"
			allowEmpty="true" weight="175" defaultValue="" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall,dictionary.authentication" visibilityDependsValue="true,true,BASIC|NTLM"/>/>
		<property resourceKey="dictionary.logonMethod" inputType="select"
			options="GET, POST" weight="200" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
		<property resourceKey="dictionary.logonUrl" inputType="text"
			defaultValue="" weight="300" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
		<property resourceKey="dictionary.logonVariables" inputType="namePairs"
			weight="500" urlEncode="false" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
		<property resourceKey="dictionary.logonHeaders" inputType="namePairs"
			weight="600" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
		<property resourceKey="dictionary.logonResponseList" inputType="multipleTextInput"
			weight="700" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"
			defaultValue="200"/>
		<property resourceKey="dictionary.logonResponseContent" inputType="text"
			defaultValue="" weight="800" visibilityDependsOn="dictionary.logonRequired,dictionary.blacklistApiCall" visibilityDependsValue="true,true"/>
	</propertyCategory>
		
</propertyTemplates>